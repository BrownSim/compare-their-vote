<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}CompareTheirVotes.eu{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <meta name="description" content="{{ 'meta.description'|trans }}"/>
        <link rel="icon" href="{{ asset('images/fav.webp') }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body class="{{ bodyclass|default('') }}">
        <header class="main-header">
            {{ include('_header.html.twig') }}
        </header>
        <main>
            {% block body %}{% endblock %}
        </main>
        {{ include('_footer.html.twig') }}
    </body>
</html>
