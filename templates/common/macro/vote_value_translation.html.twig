{% macro vote_value_translation(value) %}
    {% if value == constant('App\\Entity\\MemberVote::VOTE_FOR') %}
        {{ 'vote_value.for'|trans }}
    {% elseif value == constant('App\\Entity\\MemberVote::VOTE_AGAINST') %}
        {{ 'vote_value.against'|trans }}
    {% elseif value == constant('App\\Entity\\MemberVote::VOTE_DID_NOT_VOTE') %}
        {{ 'vote_value.miss'|trans }}
    {% else %}
        {{ 'vote_value.not_vote'|trans }}
    {% endif %}
{% endmacro %}
